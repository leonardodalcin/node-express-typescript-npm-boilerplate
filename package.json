{
  "name": "warren-hot-reload",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "concurrently": "^4.1.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-jest": "^22.15.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "lerna": "^3.16.4",
    "prettier": "^1.18.2",
    "typescript": "^3.5.3"
  },
  "scripts": {
    "start": "node ./dist/index.js",
    "test": "jest",

    "lint": "eslint '*/**/*.{js,ts}' --ignore-path .gitignore --cache --fix",
    "build": "tsc -b packages/repositories packages/server packages/validators",

    "publish-warren-packages": "lerna publish",
    "list-warren-packages": "yarn workspaces info"
  },
  "dependencies": {
    "@types/jest": "^24.0.17",
    "@types/node": "^12.7.1",
    "@warreninc/repositories": "^0.0.4",
    "@warreninc/server": "^0.0.4",
    "@warreninc/validators": "^0.0.3"
  }
